    "prebuild": "npx contentlayer2 build",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "build:fast": "SKIP_ENV_VALIDATION=true NODE_OPTIONS='--max-old-space-size=4096' next build",
    "turbo": "next dev --turbo",
    "start": "next start",
    "lint": "next lint --max-warnings=0",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint:tailwind": "eslint --fix \"**/*.{js,jsx,ts,tsx}\" --rule \"tailwindcss/classnames-order: error\"",
    "preview": "next build && next start",
    "email": "email dev --dir emails --port 3333",
    "setup-env": "./update_env.sh",
    "setup-supabase": "./update_supabase.sh",
    "i18n:validate": "npx @inlang/cli@latest validate --project ./project.inlang",
    "i18n:translate": "npx @inlang/cli@latest machine translate --project ./project.inlang --locale en --targetLocales de",
    "i18n:sync": "node scripts/i18n-sync.js",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "clean": "rm -rf .next .turbo node_modules/.cache",
    "clean:all": "rm -rf .next .turbo node_modules"
