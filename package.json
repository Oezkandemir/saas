{
  "name": "cenety-monorepo",
  "version": "0.3.1",
  "private": true,
  "packageManager": "pnpm@9.15.2",
  "author": {
    "name": "mickasmt",
    "url": "https://twitter.com/mickasmt"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=@cenety/web",
    "dev:landing": "turbo run dev --filter=@cenety/landing",
    "dev:mobile": "turbo run dev --filter=@cenety/mobile",
    "expo": "cd apps/mobile && npx expo start",
    "expo:clear": "cd apps/mobile && npx expo start --clear",
    "android": "turbo run android --filter=@cenety/mobile",
    "ios": "turbo run ios --filter=@cenety/mobile",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "test": "turbo run test",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean",
    "setup-env": "./scripts/setup-env.sh",
    "setup-supabase": "./scripts/setup-supabase.sh"
  },
  "devDependencies": {
    "turbo": "^2.5.4"
  },
  "dependencies": {
    "@expo/metro-config": "^0.20.14",
    "date-fns": "^4.1.0",
    "expo": "~53.0.11",
    "metro": "^0.82.4",
    "react": "19.0.0",
    "react-native": "0.79.3"
  },
  "pnpm": {
    "overrides": {
      "react": "19.0.0",
      "react-dom": "19.0.0",
      "react-native": "0.79.3",
      "@types/react": "~19.0.14"
    }
  }
}
